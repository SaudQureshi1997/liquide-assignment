{
  "moduleFileExtensions": [
      "js",
      "json"
  ],
  "setupFilesAfterEnv": [
      "./__tests__/setup.js"
  ],
  "testRegex": "Test.js$",
  "rootDir": ".",
  "coverageDirectory": "__tests__",
  "moduleNameMapper": {
      "#root/(.*)$": "<rootDir>/$1",
      "#utils/(.*)$": "<rootDir>/utils/$1",
      "#models/(.*)$": "<rootDir>/models/$1"
    }
}